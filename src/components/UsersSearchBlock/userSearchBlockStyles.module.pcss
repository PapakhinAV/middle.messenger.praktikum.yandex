.searchBlock{
    display: flex;
    flex-direction: column;

    &__input{
        display: flex;
        flex-direction: row;
        align-items: flex-end;
        column-gap: 16px;
        padding: 8px 16px;
    }

    &__list {
        max-height: 250px;
        overflow-y: scroll;
        background-color: var(--card-background);
        border-radius: 8px;

    }
}

.hidden {
    display: none;
}
